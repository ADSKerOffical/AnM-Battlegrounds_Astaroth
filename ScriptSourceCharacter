                lastPressTime = currentTime 
                local closestDistance = math.huge
                    local closestHumanoid = nil
                    local Usuario = game.Players.LocalPlayer

                    for _, v in pairs(workspace:GetDescendants()) do
                        if v:IsA("Humanoid") and v.RootPart and v.RootPart.Parent ~= Usuario.Character then
                            local distance =
                                (v.RootPart.Position - game.Workspace.CenterHitbox.Position).magnitude
                            if distance < closestDistance and distance <= 110 then
                                closestDistance = distance
                                closestHumanoid = v
                            end
                        end
                    end

                    if closestHumanoid then
                       local args = {
           [1] = closestHumanoid.RootPart.CFrame,
           [2] = closestHumanoid.MaxHealth / 4
       }

 game:GetService("Players").LocalPlayer.Backpack["Mini Uzumaki"].FireServer.RemoteEvent:FireServer(unpack(args))
                    end
            end
end)

