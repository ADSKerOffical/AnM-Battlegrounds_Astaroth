local function aura()
local assetId = tonumber(3850926240)
  if not assetId then
   warn("Invalid asset ID")
   return
  end
local user = game.Players.LocalPlayer
local runService = game:GetService("RunService")
 
  local model = game:GetObjects("rbxassetid://" .. assetId)[1]
model.Parent = workspace
model.CanTouch = false
model.CanCollide = false
model.Size = Vector3.new(5,5,5)
model.Position = Game.Workspace[user.Name].Torso.Position + Vector3.new(0, 0, 0)

local function updateAuraPosition()
model.Position = game.Workspace[user.Name].Torso.Position + Vector3.new(0, 0, 0)
if user.Character.Humanoid.Health <= 0 then
model:Destroy()
end
end

runService.RenderStepped:Connect(updateAuraPosition)
end

local function act1()
local StarterGui = game:GetService("StarterGui")
StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Backpack, false)
game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
local Anim = Instance.new("Animation")
Anim.AnimationId = "rbxassetid://17507532908"
local k = game:GetService("Players").LocalPlayer.Character.Humanoid:LoadAnimation(Anim)
k:Play()
k:AdjustSpeed(1)
wait(0.9)
 k:AdjustSpeed(0)
local ohString1 = "Kenjaku"
game:GetService("ReplicatedStorage").Picked:FireServer(ohString1)
wait(2)
local player = game.Players.LocalPlayer
local char = player.Character or player.CharacterAdded:Wait()
local hu = char.Humanoid
local humanoidRootPart = char:WaitForChild("HumanoidRootPart")
local playerForward = humanoidRootPart.CFrame

for i = 1, 20 do
local tochk = humanoidRootPart.CFrame + Vector3.new(math.random(-100, 100), math.random(-5, 5), math.random(-100,100))
local RTy = math.random(5, 40)
    local args = {
        [1] = tochk,
        [2] = math.huge
    }
    game.Players.LocalPlayer.Backpack["Mini Uzumaki"].FireServer.RemoteEvent:FireServer(unpack(args))
end
  wait(1)
local player = game.Players.LocalPlayer
local humanoid = player.Character.Humanoid
local activeTracks = humanoid:GetPlayingAnimationTracks()

for _, track in pairs(activeTracks) do
    track:Stop()
end
coroutine.wrap(aura)()
  wait(0.5)
  game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
 local StarterGui = game:GetService("StarterGui")
StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Backpack, true)
end

