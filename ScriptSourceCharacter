local StarterGui = game:GetService("StarterGui")
StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Backpack, false)
game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
Spooky = Instance.new("Sound", game.Workspace)
Spooky.Name = "Astaroth Domain Expansion"
Spooky.SoundId = "rbxassetid://17558107142"
Spooky.Volume = 3.5
Spooky.PlaybackSpeed = 1
Spooky.Looped = false
Spooky.TimePosition = 1.5
Spooky:Play()
  wait(3)
 local function spawnUzumakiCircle(hit)
  centerPosition = hit.Parent.Humanoid.RootPart.CFrame
  local radius = 30
  local numUzumaki = 8

  for i = 1, numUzumaki do
    local angle = (i - 1) * 2 * math.pi / numUzumaki
    local x = centerPosition.X + radius * math.cos(angle)
    local y = centerPosition.Y
    local z = centerPosition.Z + radius * math.sin(angle)

    local args = {
        [1] = CFrame.new(x, y, z),
        [2] = hit.Parent.Humanoid.MaxHealth
    }

    game:GetService("Players").LocalPlayer.Backpack["Mini Uzumaki"].FireServer.RemoteEvent:FireServer(unpack(args))
  end
end

local function createBlock(position)
  local block = Instance.new("Part")
  block.Parent = workspace
  block.Size = Vector3.new(13, 13, 13)
  block.Position = position
  block.Transparency = 0.95
  block.BrickColor = BrickColor.new("Black")
  block.Color = Color3.new(0, 0, 0)
  block.Material = Enum.Material.Neon
  block.Anchored = true

  game.Debris:AddItem(block, 30)

  local fire = Instance.new("Fire")
  fire.Heat = 10
  fire.Color = block.Color
  fire.SecondaryColor = block.Color
  fire.Size = 100
  fire.Parent = block

  block.Touched:Connect(function(hit)
    if hit.Parent:FindFirstChild("Humanoid") and hit.Parent ~= game.Players.LocalPlayer.Character then
      if hit.Parent.Humanoid.Health ~= 0 then
      spawnUzumakiCircle(hit) 
    end
     end
  end)
end

local radius = 100

for i = 1, 32 do
  local angle = math.rad(i * (360 / 32))
  local x = radius * math.cos(angle)
  local z = radius * math.sin(angle)
  local position = game.Players.LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(x, 0, z)
  createBlock(position)
end
  wait(5)
k:Stop()
game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
local StarterGui = game:GetService("StarterGui")
StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Backpack, true)
            end
end)

tool.Parent = game.Players.LocalPlayer.Backpack
end

local tool = Instance.new("Tool")
tool.Name = "Astaroth"
tool.RequiresHandle = false

tool.Activated:Connect(function()
 tool:Destroy()
 coroutine.wrap(act1)()
 game.Players:Chat("/e heal")
 coroutine.wrap(tools)()
end)

tool.Parent = game.Players.LocalPlayer.Backpack
