            neonPart:Destroy()
         local explosion = Instance.new("Explosion")
explosion.BlastRadius = 0
explosion.Position = humanoid.RootPart.Position
explosion.Parent = workspace
local args = {
        [1] = hit.Parent.Humanoid.RootPart.CFrame,
