Spooky.Volume = 1
Spooky.PlaybackSpeed = 1
Spooky.Looped = false
Spooky:Play()
            end
            elseif hit.Parent:FindFirstChildOfClass("BasePart") then
          local basepar = hit.Parent:FindFirstChildOfClass("BasePart")
            local explosion = Instance.new("Explosion")
            explosion.BlastRadius = 0
            explosion.Position = basepar.Position
            explosion.Parent = workspace
        end
    end)
            end
end)

